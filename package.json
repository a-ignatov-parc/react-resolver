{
  "name": "@a-ignatov-parc/react-resolver",
  "version": "3.2.0",
  "description": "Async-rendering & data-fetching for universal React applications",
  "main": "dist",
  "engines": {
    "node": "^6.0.0",
    "yarn": "^1.1.0"
  },
  "scripts": {
    "install-deps": "npm run yarn:install && npm run yarn:install-deps",
    "yarn:install": "npm i yarn@$npm_package_engines_yarn",
    "yarn:install-deps": "yarn",
    "dist": "NODE_ENV=production babel src --out-dir dist",
    "lint": "eslint src test",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/a-ignatov-parc/react-resolver.git"
  },
  "keywords": [
    "async",
    "decorator",
    "lazy",
    "load",
    "react",
    "resolve"
  ],
  "author": "Eric Clemmons <eric@smarterspam.com>",
  "contributors": [
    "Anton Ignatov <abietis@gmail.com>"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/a-ignatov-parc/react-resolver/issues"
  },
  "homepage": "https://github.com/a-ignatov-parc/react-resolver",
  "dependencies": {
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "gitbook-cli": "^2.3.2",
    "gitbook-plugin-prism": "^2.3.0",
    "gitbook-plugin-todo": "^0.1.3",
    "mocha": "^4.0.1",
    "mocha-junit-reporter": "^1.15.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "peerDependencies": {
    "react": "^0.14.6 || ^15.0.0-0 || ^16.0.0",
    "react-dom": "^0.14.6 || ^15.0.0-0 || ^16.0.0"
  },
  "files": [
    "dist"
